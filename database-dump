CREATE MEMORY TABLE "PUBLIC"."USER"(
    "USER_NAME" CHARACTER VARYING(25) NOT NULL,
    "PASSWORD" CHARACTER VARYING(50) NOT NULL,
    "PASSWORD_SALT" CHARACTER VARYING(50) NOT NULL,
    "FULL_NAME" CHARACTER VARYING(50) NOT NULL,
    "SESSION_ID" CHARACTER VARYING(50)
);       
-- 0 +/- SELECT COUNT(*) FROM PUBLIC."USER";  
CREATE MEMORY TABLE "PUBLIC"."USER_SECTOR"(
    "ID" INTEGER NOT NULL,
    "USER_NAME" CHARACTER VARYING(25) NOT NULL,
    "SECTOR" INTEGER
);            
