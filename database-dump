CREATE SEQUENCE "PUBLIC"."USER_SECTOR_SEQUENCE" START WITH 1; 
CREATE MEMORY TABLE "PUBLIC"."SECTOR"(
    "ID" INTEGER NOT NULL,
    "DESCRIPTION" CHARACTER VARYING(50) NOT NULL,
    "PARENT_SECTOR" INTEGER
);        
